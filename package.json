    "cross-env": "7.0.3",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "glob": "7.1.7",
    "node-dev": "7.0.0",
    "node-sass": "6.0.0",
    "npm-run-all": "4.1.5",
